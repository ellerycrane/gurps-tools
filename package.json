  "description": "GURPS Tools",
  "license": "UNLICENSED",
  "scripts": {
    "test": "mocha --compilers js:babel-register test/**/*Tests.js",
    "build": "npm run clean && webpack --colors",
    "development": "npm run clean && ./scripts/start-development.sh",
    "lint": "eslint ./src && ./node_modules/stylelint/bin/stylelint.js './src/**/*.scss' --syntax scss",
    "fix": "eslint ./src --fix",
    "clean": "rm -rf dist",
    "start": "npm run start:local",
    "start:local": "APP_BASE_PATH=http://localhost:8003 PORT=8003 npm run development",
    "deploy": "gh-pages -d dist",
    "build-deploy": "npm run build && gh-pages -d dist"
  },
  "contributors": [
    {
      "name": "Ellery Crane"
    }
  ],
